package dec;

public class Demo{
	public static void main(String[] args) {
		getSumber(SPRING);
	}
	
	
	public static void getSumber(Sumber s) {
		switch (s) {
		case SPRING:
			System.out.println("getSumber()-春天");
			break;
		case SUMMBER:
			System.out.println("getSumber()-夏天");
			break;
		case AUTUMN:
			System.out.println("getSumber()-秋天");
			break;
			
		default:
			System.out.println("getSumber()-冬天");
			break;
		}
	}
}