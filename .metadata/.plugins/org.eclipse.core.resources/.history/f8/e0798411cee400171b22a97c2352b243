package Swing;

import javax.swing.JFrame;

public class JPanelDemo extends JFrame{
	public JPanelDemo() {
		// TODO Auto-generated constructor stub
		setBounds(100, 100, 500, 300);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		setVisible(true);
	}
	
	public static void main(String[] args) {
		new JPanelDemo();
	}
}
