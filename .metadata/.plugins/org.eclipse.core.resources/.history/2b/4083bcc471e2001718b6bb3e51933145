package dec;

public class Demo {
	public static void main(String[] args) {
		
	}

	
	public void doit(Case c) {
		switch (c) {
		case MONDAY:
			System.out.println("doit()星期一");
			break;
		case SATDAY:
			System.out.println("doit()星期二");
			break;
		default:
			System.out.println("doit()不知道");
			break;
		}
	      
	}
	public class UseParameter implements SetParameter{
		public void getParameter() {
			System.out.println("使用接口成功");
		}
	}
}
